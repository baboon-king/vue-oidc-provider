{
  "name": "vue-oidc-provider",
  "version": "0.0.1",
  "description": "OpenID Connect & OAuth2 authentication using vue provider api as state management",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-oidc-provider.umd.js",
  "module": "./dist/vue-oidc-provider.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-oidc-provider.js",
      "require": "./dist/vue-oidc-provider.umd.js"
    }
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "format": "prettier --write .",
    "build": "pnpm vite build",
    "prepack": "pnpm build",
    "build:types": "pnpm tsc --emitDeclarationOnly",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish"
  },
  "keywords": [
    "vue",
    "vue3",
    "vue-oidc-provider",
    "authentication",
    "oauth2",
    "oidc",
    "openid",
    "OpenID Connect",
    "react-provider"
  ],
  "author": "BaboonKing",
  "peerDependencies": {
    "oidc-client-ts": "^2.0.0",
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.6.5",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "oidc-client-ts": "^2.0.5",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vite-plugin-dts": "^1.4.1",
    "vue": "^3.2.37"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "license": "MIT",
  "packageManager": "pnpm@7.8.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
